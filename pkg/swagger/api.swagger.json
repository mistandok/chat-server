    }
  },
  "tags": [
    {
      "name": "ChatV1"
    }
  ],
  "host": "localhost:8051",
  "schemes": [
    "http",
    "https"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/chat/v1": {
      "delete": {
        "summary": "Удаляет чат",
        "operationId": "ChatV1_Delete",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "id",
            "description": "Идентификатор чата",
            "in": "query",
            "required": false,
            "type": "string",
            "format": "int64"
          }
        ],
        "tags": [
          "ChatV1"
        ]
      },
      "post": {
        "summary": "Создает новый чат с указанными пользователями",
        "operationId": "ChatV1_Create",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "$ref": "#/definitions/chat_v1CreateResponse"
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/chat_v1CreateRequest"
            }
          }
        ],
        "tags": [
          "ChatV1"
        ]
      }
    },
    "/chat/v1/send-message": {
      "post": {
        "summary": "Отправляет сообщение от пользователя в указанный чат",
        "operationId": "ChatV1_SendMessage",
        "responses": {
          "200": {
            "description": "A successful response.",
            "schema": {
              "type": "object",
              "properties": {}
            }
          },
          "default": {
            "description": "An unexpected error response.",
            "schema": {
              "$ref": "#/definitions/rpcStatus"
            }
          }
        },
        "parameters": [
          {
            "name": "body",
            "in": "body",
            "required": true,
            "schema": {
              "$ref": "#/definitions/chat_v1SendMessageRequest"
            }
          }
        ],
        "tags": [
          "ChatV1"
        ]
      }
    }
  },
  "definitions": {
    "chat_v1CreateRequest": {
      "type": "object",
      "properties": {
        "userIDs": {
          "type": "array",
          "items": {
            "type": "string",
            "format": "int64"
          },
          "title": "Идентификаторы пользователей в чате"
        }
      }
    },
    "chat_v1CreateResponse": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string",
          "format": "int64",
          "title": "Идентификатор чата"
        }
      }
    },
    "chat_v1SendMessageRequest": {
      "type": "object",
      "properties": {
        "from": {
          "type": "string",
          "format": "int64",
          "title": "Идентификатор пользователя, отправляющего сообщение"
        },
        "text": {
          "type": "string",
          "title": "Текст сообщения"
        },
        "timestamp": {
          "type": "string",
          "format": "date-time",
          "title": "Время сообщения"
        },
        "toChatId": {
          "type": "string",
          "format": "int64",
          "title": "Идентификатор чата, в который адресовано сообщений"
        }
      }
    },
    "protobufAny": {
      "type": "object",
      "properties": {
        "@type": {
          "type": "string"
        }
      },
      "additionalProperties": {}
    },
    "rpcStatus": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer",
          "format": "int32"
        },
        "message": {
          "type": "string"
        },
        "details": {
          "type": "array",
          "items": {
            "type": "object",
            "$ref": "#/definitions/protobufAny"
          }
        }
      }
    }
  }
}
